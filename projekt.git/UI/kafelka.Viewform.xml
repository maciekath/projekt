    <Field EditValue="{Pracownik}" CaptionHtml="Pozycja kontraktu" Width="40" />
    <Field EditValue="{brunch }" CaptionHtml="Nr dokumentu" Width="40" />
    <Field EditValue="{data}" CaptionHtml="Data dokumentu" Width="40" />
    
  </Grid>
</DataForm>

